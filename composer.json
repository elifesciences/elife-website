{
    "name": "elife/elife",
    "description": "The open-access eLife journal.",
    "license": "proprietary",
    "require": {
        "predis/predis": "~1.0",
        "wikimedia/composer-merge-plugin": "~1.3"
    },
    "extra": {
        "merge-plugin": {
            "include": [
                "./src/elife_profile/modules/*/*/composer.json",
                "./tests/*/composer.json"
            ]
        }
    },
    "config": {
        "bin-dir": "bin/",
        "platform": {
            "php": "5.6"
        }
    }
}
